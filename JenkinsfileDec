pipeline {
    agent any

    stages {
        stage ('Compile Stage') {
            steps {
                echo  'Compile Stage start'
                  #ssh "echo  $PATH"
                echo  'Compile Stage end'
            }
        }

        stage ('Testing Stage') {
            steps {
                echo  'mvn test'
            }
        }

        stage ('installing Stage') {
            steps {
                    echo  'mvn install'
            }
        }

        stage ('Deployment Stage') {
            steps {
                echo  'mvn deploy'
            }
        }
    }
}
