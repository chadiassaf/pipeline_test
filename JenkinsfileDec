pipeline {
    agent any
    
    environment {
      whoAmIVar = sh returnStdout: true, script: 'whoami'
    }

    stages {
        stage ('Compile Stage') {
            steps {
                echo  'Compile Stage start'
                echo  "${PATH}"
                echo ${whoAmIVar}"
                echo  'Compile Stage end'
            }
        }

        stage ('Testing Stage') {
            steps {
                echo  'mvn test'
            }
        }

        stage ('installing Stage') {
            steps {
                    echo  'mvn install'
            }
        }

        stage ('Deployment Stage') {
            steps {
                echo  'mvn deploy'
            }
        }
    }
}

def whoAmIFunc()
{
    def wxx =  sh returnStdout: true, script: 'whoami'
    return wxx
}
