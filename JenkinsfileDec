pipeline {
    agent any
    tools {
      git 'Default'
    }

    stages {
        stage ('Compile Stage') {
            steps {
                    echo  $PATH
            }
        }

        stage ('Testing Stage') {
            steps {
                echo  'mvn test'
            }
        }

        stage ('installing Stage') {
            steps {
                    echo  'mvn install'
            }
        }

        stage ('Deployment Stage') {
            steps {
                echo  'mvn deploy'
            }
        }
    }
}
